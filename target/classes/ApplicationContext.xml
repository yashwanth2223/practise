<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
        
        
        <bean id="foodbean" class="Autow.Food">
        
        <constructor-arg value="Vegsandwich"></constructor-arg>
         <constructor-arg value="Muskmelon"></constructor-arg>
         <constructor-arg value="Cheesepizza"></constructor-arg>
         <constructor-arg value="vegburger"></constructor-arg>
         <constructor-arg ref="burgerbean"></constructor-arg>
        </bean>
        
        <bean id="burgerbean" class="Autow.Burgert">
        <constructor-arg value="mixedcheese"></constructor-arg>
         <constructor-arg value="mexicancheese"></constructor-arg>
          <constructor-arg value="McChicken Burger"></constructor-arg>
          <constructor-arg value="Falafel burgers"></constructor-arg>
        </bean>
        
        
   </beans>     